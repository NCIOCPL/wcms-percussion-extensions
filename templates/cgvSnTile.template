    <bindings>
      <binding id="2">
        <execution-order>1</execution-order>
        <expression>$sys.variables.rxs_navbase</expression>
        <id>0</id>
        <jexl-script id="3">
          <text>$sys.variables.rxs_navbase;</text>
        </jexl-script>
        <variable>$rxs_navbase</variable>
      </binding>
      <binding id="4">
        <execution-order>2</execution-order>
        <expression>$rx.location.generate($sys.assemblyItem, "nciBnImage")</expression>
        <id>0</id>
        <jexl-script id="5">
          <text>$rx.location.generate($sys.assemblyItem, "nciBnImage");</text>
        </jexl-script>
        <variable>$link</variable>
      </binding>
      <binding id="6">
        <execution-order>3</execution-order>
        <expression>$sys.variables.rxs_imageBase </expression>
        <id>0</id>
        <jexl-script id="7">
          <text>$sys.variables.rxs_imageBase;</text>
        </jexl-script>
        <variable>$rxs_imageBase</variable>
      </binding>
    </bindings>
